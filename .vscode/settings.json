{
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascript]": {
		"editor.defaultFormatter": "oxc.oxc-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[css]": {
		"editor.defaultFormatter": "bradlc.vscode-tailwindcss"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "oxc.oxc-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[cairo]": {
		"editor.defaultFormatter": "StarkWare.cairo1"
	},
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"editor.formatOnSave": true,
	"editor.tabSize": 2,
	"search.exclude": {
		"**/*.bin": true,
		"**/*.gltf": true,
		"**/*.jpg": true,
		"**/*.typeface.json": true,
		"**/*.wav": true,
		"**/*.webp": true,
		"**/*.webm": true,
		"**/*.glb": true,
		"**/*.png": true,
		"**/*.cube": true,
		"**/*.CUBE": true,
		"**/*.mp3": true,
		"**/*.hdr": true,
		"**/*.woff": true,
		"**/*.woff2": true,
		"**/*.ttf": true,
		"**/*.svg": true,
		"**/*.tga": true,
		"**/*.fbx": true
	},
	"cairo1.enableLanguageServer": true,
	"cairo1.languageServerPath": "${userHome}/.asdf/installs/dojo/1.3.1/bin/dojo-language-server",
	"cairo1.enableScarb": true,
	"cairo1.scarbPath": "${userHome}/.asdf/installs/scarb/2.9.2/bin/scarb",
	"cairo1.preferScarbLanguageServer": false,
	"typescript.tsdk": "node_modules/typescript/lib",
	"oxc.enable": true,
	"oxc.configPath": "packages/client/.oxlintrc.json",
	"biome.enabled": true,
	"npm.packageManager": "bun",
	"npm.scriptRunner": "bun"
}
