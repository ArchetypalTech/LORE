		"clsx": "^2.1.1",
		"dnd-kit-tree": "^1.0.22",
		"envalid": "^8.0.0",
		"immer": "^10.1.1",
		"json-bigint": "^1.0.0",
		"lucide-react": "^0.487.0",
		"pure-rand": "^7.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"sonner": "^2.0.3",
		"starknet": "^6.23.1",
		"tailwind-merge": "^3.0.1",
