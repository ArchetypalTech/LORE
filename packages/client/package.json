{
	"name": "@lore/client",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "bunx vite build",
		"build:slot": "bunx vite build --mode slot",
		"dev": "bunx vite --mode development",
		"dev:slot": "bunx vite dev --mode slot",
		"lint": "bun run lint:oxlint && bun run lint:biome",
		"lint:biome": "biome check --write",
		"lint:ci": "bun run lint:ci:oxlint && bun run lint:ci:biome",
		"lint:ci:biome": "biome ci",
		"lint:ci:oxlint": "bunx oxlint -c ./.oxlintrc.json --import-plugin",
		"lint:oxlint": "bunx oxlint -c ./.oxlintrc.json --import-plugin --fix",
		"preview": "bunx vite preview --port 8080",
		"start": "bunx serve -s /dist --port 8080"
	},
	"dependencies": {
		"@cartridge/controller": "^0.7.4",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
